script/source = "extends CharacterBody2D

class_name Enemy
@export var speed = 100
@export var health = 100
@export var jumpRate = 1000
@export var jumpDistance = 200
@export var knockBackForce = 1000
@export var slimeValue = 1
@export var damage = 1

@onready var enemySprite : Sprite2D = $Enemies
@onready var animationPlayer : AnimationPlayer = $AnimationPlayer
@onready var animationTree : AnimationTree = $AnimationTree
@onready var navAgent : NavigationAgent2D = $NavigationAgent2D


var knockBack: Vector2 = Vector2.ZERO;
var stateMachine : AnimationNodeStateMachinePlayback
var sprites = []
var jumpDirection = null
func _ready():
	
	var path = \"res://Sprites/Slimes\"
	
	var dir = DirAccess.open(path)
	
	if dir:
		dir.list_dir_begin()
		var file_name = dir.get_next()
		while file_name != \"\":
			if dir.current_is_dir():
				print(\"Found directory: \" + file_name)
			elif !file_name.ends_with('import') && file_name.begins_with('slime'):
				print(\"Found file: \" + file_name)
				sprites.append(load(path + '/'+ file_name))
			file_name = dir.get_next()
	else:
		print(\"An error occurred when trying to access the path.\")
	
	randomize()
	
	animationTree.active = true
	print(sprites)
#	enemySprite.texture = sprites[randi_range(0, len(sprites)-1)]
	
	stateMachine = animationTree.get('parameters/playback')
func _physics_process(delta):
	if	health <= 0:
		stateMachine.travel(\"Die\")
		return
		
	var currentState : String = stateMachine.get_current_node()
	match currentState:
		'Stunned':
			velocity = -knockBack
			jumpDirection = null
		'Jump':
			if jumpDirection == null:
				jumpDirection = Globals.Player.global_position - global_position
				
			velocity = jumpDirection 
		'Idle':
			randomize()
			
			var jump = randi_range(0, jumpRate)
			
			jumpDirection = null
			
			var distanceToPlayer = global_position.distance_to(Globals.Player.global_position)
	
			if 	jump == 1 && distanceToPlayer <= jumpDistance:
				stateMachine.travel('Jump')
				return
			
			var playerDirection = Globals.Player.global_position - global_position
			
			velocity = playerDirection.normalized() * speed
		_:
			print(currentState, 'no match found')
	if	velocity.x < 0:
		enemySprite.flip_h = true
	elif velocity.x > 0:
		enemySprite.flip_h = false
	
	if 	knockBack != Vector2.ZERO:
		knockBack = Vector2.ZERO
	print(velocity)
	move_and_slide()
	
func receiveDamage(damage):
	knockBack = -global_position.direction_to(Globals.Player.position) * damage * knockBackForce
	
	health -= damage
	
	stateMachine.travel(\"Stunned\")
	

func die():
	Globals.slimeJuice += slimeValue
	queue_free()
"

